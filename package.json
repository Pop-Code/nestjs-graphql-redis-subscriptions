{
    "name": "nestjs-graphql-redis-subscriptions",
    "version": "0.0.6",
    "description": "A nestjs module that provide an instance of graphql-redis-subscriptions",
    "author": "Alex Hermann <contact@pop-code.com>",
    "repository": "https://github.com/Pop-Code/nestjs-graphql-redis-subscriptions.git",
    "license": "MIT",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "rm -Rf dist && tsc -b tsconfig.build.json",
        "format": "prettier \"**/*.ts\" --ignore-path ./.prettierignore --write && git status",
        "lint": "eslint ./src/**/*.ts",
        "doc": "rm -Rf ./docs && typedoc ./src && touch ./docs/.nojekyll",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
    },
    "peerDependencies": {
        "@nestjs/common": "^7 || ^8",
        "graphql": "^15.5.0"
    },
    "dependencies": {
        "graphql-redis-subscriptions": "2.4.0",
        "graphql-subscriptions": "1.2.1",
        "ioredis": "4.27.8"
    },
    "devDependencies": {
        "@nestjs/common": "8.0.6",
        "@nestjs/core": "8.0.6",
        "@nestjs/platform-express": "8.0.6",
        "@nestjs/testing": "8.0.6",
        "@types/jest": "27.0.1",
        "@typescript-eslint/eslint-plugin": "4.29.3",
        "@typescript-eslint/parser": "4.29.3",
        "@types/node": "16.11.6",
        "codecov": "3.8.3",
        "eslint": "7.32.0",
        "eslint-plugin-import": "2.24.1",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-config-prettier": "8.3.0",
        "jest": "27.0.6",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.3.0",
        "ts-jest": "27.0.5",
        "ts-node": "10.2.1",
        "tsconfig-paths": "3.10.0",
        "typedoc": "0.21.6",
        "typescript": "4.3.5",
        "@types/ioredis": "4.26.7",
        "graphql": "15.5.1",
        "prettier": "2.3.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.{js,jsx,ts}",
            "!index.ts",
            "!**/test/**"
        ],
        "coverageDirectory": "../coverage"
    },
    "engines": {
        "node": ">=0.12"
    }
}
